{
  "version": 2,
  "builds": [
    {
      "src": "/client",
      "use": "@vercel/static-build"
    },
    {
      "src": "/server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/server/$1" },
    { "src": "/(.*)", "dest": "/client/$1" ,"headers": { "Cache-Control": "s-maxage=0" }}
  ]
}

